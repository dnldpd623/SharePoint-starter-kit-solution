    <pnp:ContentDeliveryNetwork>
      <pnp:Public Enabled="true">
        <pnp:Origins>
          <pnp:Origin Url="*/CLIENTSIDEASSETS" Action="Add"/>
        </pnp:Origins>
      </pnp:Public>
    </pnp:ContentDeliveryNetwork>
  </pnp:Tenant>
